{% extends 'base.html.twig' %}

{% block title %}Student Register{% endblock %}

{% block body %}
    <main class="valign-wrapper">

        <div class="container">
            <div class="row white z-depth-4" style="padding: 10px; border-radius: 10px;">

                {{ form_start(form) }}
                <div class="row">
                    <div class="input-field">
                        <p class="red-text">  {{ form_errors(form.course) }}</p>
                        {{ form_widget(form.course) }}
                    </div>
                </div>
                {{ form_end(form) }}

            </div>

        </div>


    </main>

{% endblock %}
